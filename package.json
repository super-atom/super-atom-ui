    "prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --mode production --progress",
    "pretty": "prettier --write \"./**/*.{ts,js,scss,pug,json}\"",
    "clean": "rimraf ./dist/*",
    "create": "node scripts/create-page.js",
    "build": "npm-run-all clean prod",
    "deploy": "netlify deploy --prod"
